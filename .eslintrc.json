{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:tailwindcss/recommended"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "jsx-quotes": ["error", "prefer-single"],
    "react/jsx-props-no-spreading": "off",
    "react/jsx-one-expression-per-line": "off",
  }
}
